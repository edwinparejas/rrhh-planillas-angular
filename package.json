{
  "name": "rrhh-personal-web",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-max": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --dev",
    "build-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
    "build-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-staging": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=staging",
    "build-prod-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
   
    "build-prod-testing": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration=testing",
    "build-prod-desarrollo": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.1",
    "@angular/cdk": "9.2.4",
    "@angular/common": "10.1.1",
    "@angular/compiler": "10.1.1",
    "@angular/core": "10.1.1",
    "@angular/flex-layout": "9.0.0-beta.31",
    "@angular/forms": "10.1.1",
    "@angular/material": "9.2.4",
    "@angular/material-moment-adapter": "9.2.4",
    "@angular/platform-browser": "10.1.1",
    "@angular/platform-browser-dynamic": "10.1.1",
    "@angular/router": "10.1.1",
    "crypto-js": "3.3.0",
    "file-saver": "2.0.2",
    "form-data": "~2.3.2",
    "hammerjs": "2.0.8",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "2.26.0",
    "ngx-spinner": "9.0.2",
    "perfect-scrollbar": "1.5.0",
    "rxjs": "6.5.5",
    "secure-web-storage": "1.0.2",
    "sweetalert2": "9.13.4",
    "tslib": "1.13.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "10.1.1",
    "@angular/compiler-cli": "10.1.1",
    "@angular/language-service": "10.1.1",
    "@angularclass/hmr": "2.1.3",
    "@types/jasmine": "3.3.14",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.136",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "ts-node": "7.0.1",
    "tslint": "5.15.0",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "3.3.2"
  }
}
